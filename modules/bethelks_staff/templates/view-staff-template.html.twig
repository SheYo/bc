{% if staff is null %}
  <p>The staff member you are looking for does not exist.</p>
{% else %}
  <div>Name:</div>
  <div>{{staff.name}}</div>

  <div>Category:</div>
  <div>{{staff.staff_category}}</div>

  <div>Position:</div>
  <div>{{staff.position}}</div>

  <div>Telephone:</div>
  <div>{{staff.telephone}}</div>

  <div>Extension:</div>
  <div>{{staff.extension}}</div>

  <div>Location:</div>
  <div>{{staff.campus_location}}</div>

  <div>Email:</div>
  <div><a href="mailto:{{staff.email}}">{{staff.email}}</a></div>

  <div>Biography:</div>
  <div>{{staff.bio}}</div>

  <div>
  {% if staff.imguri is not null %}
    <img src="{{ file_url(staff.imguri) }}">
  {% endif %}
  </div>
{% endif %}
